<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
    <GenerateTargetFrameworkAttribute>false</GenerateTargetFrameworkAttribute>
    <AWSProjectType>Lambda</AWSProjectType>

    <!-- This property makes the build directory similar to a publish directory and helps the AWS .NET Lambda Mock Test Tool find project dependencies. -->
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Amazon.Lambda.APIGatewayEvents" Version="2.4.0" />
    <PackageReference Include="Amazon.Lambda.CloudWatchEvents" Version="2.1.0" />
    <PackageReference Include="Amazon.Lambda.Core" Version="1.2.0" />
    <PackageReference Include="Amazon.Lambda.Serialization.SystemTextJson" Version="2.1.0" />
    <PackageReference Include="AWSSDK.ApiGatewayManagementApi" Version="3.7.0.11" />
    <PackageReference Include="AWSSDK.CloudFormation" Version="3.7.2.8" />
    <PackageReference Include="AWSSDK.CloudWatchLogs" Version="3.7.0.21" />
    <PackageReference Include="AWSSDK.DynamoDBv2" Version="3.7.3.46" />
    <PackageReference Include="AWSSDK.ECS" Version="3.7.0.14" />
    <PackageReference Include="AWSSDK.EventBridge" Version="3.7.5.23" />
    <PackageReference Include="AWSSDK.GameLift" Version="3.7.0.16" />
    <PackageReference Include="AWSSDK.S3" Version="3.7.0.25" />
    <PackageReference Include="AWSSDK.StepFunctions" Version="3.7.0.168" />
    <PackageReference Include="CompareNETObjects" Version="4.73.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>
</Project>
